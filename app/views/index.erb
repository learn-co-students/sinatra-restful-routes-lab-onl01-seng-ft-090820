<h1>All Recipes</h1>
<% @recipes.each do |recipe|%>
    <h2><%= recipe.name %></h2>
    <p><%= recipe.ingredients %></p>
    <p><%= recipe.cook_time %></p>
    binding.pry
    <a href="/recipes/<%= recipe.id %>">See recipe</a>
<% end%>
<form method="GET" action="/recipes/new">
<input type="submit" value="Add New Recipe">
</form>